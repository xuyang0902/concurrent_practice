<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="app" class="com.zx.yuren.spring.beanInit.App" init-method="init">
        <property name="version" value="2"/>
        <property name="appName" value="appName"/>
    </bean>

    <bean class="com.zx.yuren.spring.beanInit.BeanPostProcessor1"/>
    <bean class="com.zx.yuren.spring.beanInit.BeanFactoryPostProcessor1"/>
    <bean class="com.zx.yuren.spring.beanInit.BeanDefinitionRegistryPostProcessor1"/>

</beans>